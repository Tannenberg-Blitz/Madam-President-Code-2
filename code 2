construct = (a = 1) => {
	
    e.page = e.page + a < e.pages.length ? e.page + a : 0
    let html = e.header;
    html += e.pages[e.page] + "<br>";
    if (e.page > 0) {
    
    html += `<button style="color: aliceblue; background-color: #223b61;" onclick="endingConstructor(-1)">Return</button>`
    }
    if (e.page < e.pages.length - 1) {
    
    html += `<button style="color: aliceblue; background-color: #223b61;" onclick="endingConstructor(1)">Continue</button>`
    }
    
    for (i in e.executable) {
    if (e.executable[i][0] == e.page) {
        e.executable[i][1]();
        
    }
    }
    
