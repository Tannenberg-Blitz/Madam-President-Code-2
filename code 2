        case "Default":
        break;
        case "Ted Cruz":
            keepCruz();
        break;
        case "Marco Rubio":
            switchToRubio();
        break;
        case "Donald Trump":
            switchToTrump();
        break;
        case "Rand Paul":
            switchToPaul();
        break;
        case "Chris Christie":
            switchToChristie();
        break;
        case "Nikki Haley":
            switchToHaley();
        break;
        case "Larry Hogan":
            switchToHogan();
        break;
        case "Lindsay Graham":
            switchToGraham();
        break;
