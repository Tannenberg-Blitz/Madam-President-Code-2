        var t = (e.target && e.target.dataState) || e.dataState;
        return this._getState(t);
      },
      _getState: function (e) {
        var t = this.stateShapes[e];
        var n = this.stateHitAreas[e];
        var r = this.labelShapes[e];
        var i = this.labelTexts[e];
