    midterms += 1;
}
switch (midterms) {
    case -1:
        midtermsWin = "default";
    break;
    case 0:
        midtermsWin = "lose";
    break;
    case 1:
        midtermsWin = "lose";
    break;
    case 2:
        midtermsWin = "lose";
    break;
    case 3:
        midtermsWin = "lose";
    break;
    case 4:
        midtermsWin = "lose";
    break;
    case 5:
        midtermsWin = "lose";
    break;
    case 6:
        midtermsWin = "lose little";
    break;
    case 7:
        midtermsWin = "lose little";
    break;
    case 8:
        midtermsWin = "lose little";
    break;
    case 9:
        midtermsWin = "lose little";
    break;
