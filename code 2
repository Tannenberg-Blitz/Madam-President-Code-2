function keepCruz() {
    e.candidate_json[1].fields.color_hex = '#910d16'
    if (tedCruz == false){
    tedCruz = true;
    applyDrift(131,-0.001);
    campaignTrail_temp.candidate_json[1].fields.first_name = "Ted"; 
    campaignTrail_temp.candidate_json[1].fields.last_name = "Cruz";

    e.questions_json[26].fields.description = Cruz[0];
    e.answers_json[80].fields.description = Cruz[1];
    e.answers_json[81].fields.description = Cruz[2];
    e.answers_json[82].fields.description = Cruz[3];
    e.answers_json[83].fields.description = Cruz[4];
    e.answer_feedback_json[80].fields.answer_feedback = Cruz[5];
    e.answer_feedback_json[81].fields.answer_feedback = Cruz[6];
    e.answer_feedback_json[82].fields.answer_feedback = Cruz[7];
    e.answer_feedback_json[83].fields.answer_feedback = Cruz[8];

    e.questions_json[28].fields.description = Cruz[9];
    e.answers_json[87].fields.description = Cruz[10];
    e.answers_json[88].fields.description = Cruz[11];
    e.answers_json[89].fields.description = Cruz[12];
    e.answers_json[90].fields.description = Cruz[13];
    e.answer_feedback_json[87].fields.answer_feedback = Cruz[14];
